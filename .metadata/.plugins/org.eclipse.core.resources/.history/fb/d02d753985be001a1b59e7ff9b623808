package CodeChefJulyLong2020;

import java.util.Scanner;

public class ChefAndCardGame 
{
	int getSumOfDigits(int number)
	{
		int result = 0;
		while(number%10 != 0)
		{
			result+=result%10;
		}
		return result;
	}
	
	public static void main(String[] args) 
	{
		System.out.println("Start...");
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-->0)
		{
			int n = s.nextInt();
			int ta=0, tb=0;
			while(n-->0)
			{
				int na = s.nextInt();
				int nb = s.nextInt();
			}
		}
		s.close();
	}

}
